<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicplayerapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".EdmActivity"
            android:label="@string/edm"
            android:parentActivityName=".MainActivity" />

        <activity
            android:name=".Top40Activity"
            android:label="@string/top_40"
            android:parentActivityName=".MainActivity" />

        <activity
            android:name=".JpopActivity"
            android:label="@string/jpop"
            android:parentActivityName=".MainActivity" />

        <activity
            android:name=".TurkishPopActivity"
            android:label="@string/turkish_pop"
            android:parentActivityName=".MainActivity" />

        <activity
            android:name=".SongDetailsPage"
            android:label="Song Details" />

    </application>


</manifest>